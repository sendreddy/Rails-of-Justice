<p id="notice"><%= notice %></p>

<h1>Registrants</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Webinar</th>
      <th>County</th>
       <th>CLE ID</th>
      <th>Job</th>
      <th>Listserv</th>
      <th>Topics</th>
      <th>Paid</th>
      <th>Amount</th>
      <th colspan="3"></th>

    </tr>
  </thead>

  <tbody>
    <% @registrants.each do |registrant| %>
      <tr>
        <td><%= registrant.first_name %></td>
        <td><%= registrant.last_name %></td>
        <td><%= registrant.email %></td>
        <td><%= registrant.webinar %></td>
        <td><%= registrant.county %></td>
        <td><%= registrant.cleID %></td>
        <td><%= registrant.job %></td>
        <td><%= registrant.listserv %></td>
        <td><%= registrant.topics %></td>
        <td><%= registrant.paid %></td>
        <td><%= registrant.amount %></td>
        <td><%= link_to 'Show', registrant %></td>
        <td><%= link_to 'Edit', edit_registrant_path(registrant) %></td>
        <td><%= link_to 'Destroy', registrant, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registrant', new_registrant_path %>
